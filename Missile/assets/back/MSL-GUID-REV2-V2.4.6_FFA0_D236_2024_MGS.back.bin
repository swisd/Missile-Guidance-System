    explosive_mass = 300
    shrapnel = False
    if anti == 'yes':
        anti_tank: bool = True
    else:
        anti_tank = False
    warhead = "HEAT"
    print("Detonated. Explosive Mass of", explosive_mass, "Lbs Hit target successfully with", warhead,
          "warhead. ", "Shrapnel:",
          shrapnel, "  AT/AS:", anti_tank, "  Missile:", missile, "  Altitude:", alt)


missilePosition = position
